            android:layout_width="235dp"
            android:layout_height="38dp"
            android:layout_marginStart="10dp"
            android:layout_toEndOf="@+id/textView8"
            android:background="@xml/okrugl_textinput"
            android:gravity="top"
            android:padding="8sp"
            android:textColor="#757575"
            android:textSize="11sp"
            tools:ignore="TextContrastCheck,DuplicateSpeakableTextCheck,DuplicateClickableBoundsCheck,SpeakableTextPresentCheck,TouchTargetSizeCheck" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/relativeLayout11"
        android:layout_width="match_parent"
        android:layout_height="56dp"
        android:layout_marginTop="0dp"
        app:layout_constraintTop_toBottomOf="@+id/relativeLayout10"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="0dp">

        <TextView
            android:id="@+id/textView10"
            android:layout_width="90dp"
            android:layout_height="40dp"
            android:layout_centerVertical="true"
            android:layout_marginStart="22sp"
            android:text="@string/localtimelabel"
            android:textSize="16sp" />

        <com.google.android.material.textfield.TextInputEditText

            android:id="@+id/textInputEditText8"
            android:layout_width="235dp"
            android:layout_height="38dp"
            android:layout_alignParentTop="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="62dp"
            android:layout_marginTop="7dp"
            android:layout_marginBottom="11dp"
            android:layout_toEndOf="@+id/textView10"
            android:background="@xml/okrugl_textinput"
            android:gravity="top"
            android:padding="8sp"
            android:textColor="#757575"
            android:textSize="11sp"
            tools:ignore="TextContrastCheck,DuplicateSpeakableTextCheck,DuplicateClickableBoundsCheck,SpeakableTextPresentCheck,TouchTargetSizeCheck" />
    </RelativeLayout>

    <com.google.android.material.button.MaterialButton
        android:id="@+id/sohranit"

        android:layout_width="253dp"
        android:layout_height="51dp"
        android:layout_marginTop="40dp"
        android:text="@string/refreshlabel"
        android:textColor="#2E7D32"
        android:textSize="13sp"
        app:backgroundTint="#009BFF85"
        app:cornerRadius="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/relativeLayout8"
        app:layout_constraintVertical_bias="0.0"
        tools:ignore="DuplicateSpeakableTextCheck" />

</androidx.constraintlayout.widget.ConstraintLayout>